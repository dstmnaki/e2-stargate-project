@name Naki's Stargates V5.0 - Stargate - SG1
@inputs Iris
@persist WebAudio Move Turn Lock2 Lock A Roll IrisActiveSend:string IrisActive CanOpen Val Switch Horizon Address:string Close WritingData:string TextEntry:string Open TargetEnt:entity Block Dial Chevron_Locked ChevsActive ErrorCode Closing V V2 Size Glyphs 
#ifdef webAudio(string)
@persist [RollA Chev OpenAudio CloseAudio Fail]:webaudio
#endif
@outputs Lock Address:string WritingData:string Horizon Open Chevron_Locked ChevsActive ErrorCode Closing Glyphs R G B

@persist Chev1 Chev2 Chev3 Chev4 Chev5 Chev6 Chev7 Chev8 Chev9 AllChev
@outputs Chev1 Chev2 Chev3 Chev4 Chev5 Chev6 Chev7 Chev8 Chev9 AllChev OK [CurAng WantedAng Ang Difference Move] DialReady Dial CanOpen

@persist [ChevronSound HorizonOpenSound HorizonCloseSound DialErrorSound]:string

@persist Width_Chevron1 Width_Chevron2 Width_Base Width [Mat_Base Mat_Chevron]:string [Color_Base Chev_Color_On Chev_Color_Off]:vector

@inputs [Glyph]:entity

@persist [CurAng WantedAng Ang Difference] WritingDataTest:string BlockRot DialReady

@model models/sprops/geometry/fdisc_132.mdl

#useable models for @model 
#models/sprops/rectangles/size_8/rect_192x192x3.mdl
#models/sprops/geometry/fdisc_132.mdl

interval(50)
runOnLast(1)

if(first()){
    #This version is still wip, you cannot dial it without datasignals, later on i will create a wireless dhd for this, with it you can use it
    #or if you know how to code, you can create your own dialing computer just like they did in the series
    
    UseWebAudio = 1
    
    #ifdef webAudio(string)
        #[
        if(RollA:isValid()){
            RollA:destroy()
        }
        if(Chev:isValid()){
            Chev:destroy()
        }
        if(OpenAudio:isValid()){
            OpenAudio:destroy()
        }
        if(CloseAudio:isValid()){
            CloseAudio:destroy()
        }
        if(Fail:isValid()){
            Fail:destroy()
        }
        ]#
        WebAudio = UseWebAudio
        
    #else
        WebAudio = 0
    #endif
    
    #the e2 uses datasignal stuff to wirelessly link the gates, meaning 
    #all you need is the address and power to go between all gates that are in the same group
    #group name is here (below) don't recommend changing it, but you can if you really want to
    dsJoinGroup("stargate-naki")
    dsSetScope(2)
    
    #include "glyph_repository"
    
    entity():setAlpha(0)
    
    Address = "spawn17"
    
    #Name of the gate
    Name = "Stargate Command"
    
    ChevronSound = "buttons/button3.wav"
